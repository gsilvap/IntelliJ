<application>
  <component name="CodeVisionSettings">
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="references" />
        <option value="vcs.code.vision" />
      </set>
    </option>
    <option name="enabled" value="false" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value=".vcs.code.vision" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern useCustomColors="true" pattern="\b(todo|fixme)\b.GSP.*">
      <option name="FOREGROUND" value="c01019" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <filter name="GSP">
      <pattern index="1" />
    </filter>
  </component>
</application>